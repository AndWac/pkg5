       "        pkgrepo [选项] 命令 [命令选项] [操作数]\n"
       "\n"
       "子命令：\n"
       "     pkgrepo create [--version] uri 或路径\n"
       "\n"
       "      pkgrepo add-signing-ca-cert [-p 发布者 ...]\n"
       "         [-s 系统信息库 uri 或路径] 路径 ...\n"
       "\n"
       "     pkgrepo add- signing-intermediate-cert [-p 发布者 ...]\n"
       "         [-s 系统信息库 uri 或路径] 路径 ...\n"
       "\n"
       "     pkgrepo get [-p 发布者  ...] [-s 系统信息库 uri 或路径]\n"
       "         [区段/属性 ...]\n"
       "\n"
       "     pkgrepo info [-F 格式] [-H] [-p 发布者 ...]\n"
       "          [-s 系统信息库 uri 或路径]\n"
       "\n"
       "     pkgrepo rebuild [-s 系统信息库 uri 或路径] [--no-catalog]\n"
       "         [--no-index]\n"
       "\n"
       "     pkgrepo refresh [-s 系统信息库 uri 或路径] [--no-catalog]\n"
       "         [--no-index]\n"
       "\n"
       "     pkgrepo remove-signing -ca-cert [-p 发布者 ...]\n"
       "         [-s 系统信息库 uri 或路径] 散列值 ...\n"
       "\n"
       "     pkgrepo remove-signing-intermediate- cert [-p 发布者 ...]\n"
       "         [-s 系统信息库 uri 或路径] 散列值 ...\n"
       "\n"
       "     pkgrepo set [-p 发布者 ...] [-s 系统信息库  uri 或路径]\n"
       "         区段/属性[+|-]=[值] ... 或\n"
       "         区段/属性[+|-]=([值]) ...\n"
       "\n"
       "     pkgrepo help\n"
       "      pkgrepo version\n"
       "\n"
       "选项：\n"
       "        --help 或 -?\n"
       "            显示用法消息。"

