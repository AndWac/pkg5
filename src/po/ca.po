       "Sub-comandes:\n"
       "        pkgdep generate [-IMm] manifest proto_dir\n"
       "        pkgdep [opcions] resolve [-dMos] manifest ...\n"
       "\n"
       "Opcions:\n"
       "        -R dir\n"
       "        --help o -?\n"
       "Entorn:\n"
       "        PKG_IMAGE"

