package SUNWhea
consolidation "osnet"
classification "System/Core"
import SUNWhea
# BEGIN NON-REDISTRIBUTABLE
drop usr/include/nfs/lm_impl.h
drop usr/include/nfs/lm_nlm.h
drop usr/include/nfs/lm_server.h
drop usr/include/sys/pcmcia/pcelx.h
$(sparc_ONLY)drop usr/include/sys/scsi/adapters/ispcmd.h
$(sparc_ONLY)drop usr/include/sys/scsi/adapters/ispmail.h
$(sparc_ONLY)drop usr/include/sys/scsi/adapters/ispreg.h
$(sparc_ONLY)drop usr/include/sys/scsi/adapters/ispvar.h
# END NON-REDISTRIBUTABLE
#
# Until 6893530 is resolved in ON, replace the affected header files with
# alternate versions.
#
drop usr/include/sys/lc_core.h
drop usr/include/sys/localedef.h
add file lc_core.h path=usr/include/sys/lc_core.h mode=0644 owner=root \
    group=bin
add file localedef.h path=usr/include/sys/localedef.h mode=0644 owner=root \
    group=bin
end package
