package BRCMbnxe
consolidation "osnet"
classification "Drivers/Networking"
import BRCMbnxe

add driver name=bnxe \
    perms="* 0666 root sys" \
    alias="pci14e4,164e" \
    alias="pci14e4,164f" \
    alias="pci14e4,1650" \
    alias="pciex14e4,164e" \
    alias="pciex14e4,164f" \
    alias="pciex14e4,1650"
end package
