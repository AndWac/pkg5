package SUNWfont-daewoo-misc
consolidation "X"
classification "System/Fonts"
import SUNWfont-daewoo-misc
add dir path=etc mode=0755 owner=root group=sys
add dir path=etc/X11 mode=0755 owner=root group=sys
add dir path=etc/X11/fontpath.d mode=0755 owner=root group=sys
add link path=etc/X11/fontpath.d/misc:unscaled:pri=30 \
    target=../../../usr/share/fonts/X11/daewoo
add link path=etc/X11/fontpath.d/75dpi:pri=75 \
    target=../../../usr/share/fonts/X11/daewoo
end package
