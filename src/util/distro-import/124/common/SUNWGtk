package SUNWGtk
consolidation "sfw"
classification "Applications/Graphics and Imaging"
import SUNWGtku
import SUNWGtkr
version 1.2.10
add dir path=usr/bin group=bin mode=0755 owner=root
add link path=usr/bin/gtk-config target=../sfw/bin/gtk-config
add dir path=usr/include mode=0755 owner=root group=bin
add link path=usr/include/gtk-1.2 target=../sfw/include/gtk-1.2
add dir path=usr/lib group=bin mode=0755 owner=root
add link path=usr/lib/libgdk-1.2.so.0 target=../sfw/lib/libgdk-1.2.so.0
add link path=usr/lib/libgdk.so target=../sfw/lib/libgdk.so
add link path=usr/lib/libgtk-1.2.so.0 target=../sfw/lib/libgtk-1.2.so.0
add link path=usr/lib/libgtk.so target=../sfw/lib/libgtk.so
add dir path=usr/share group=sys mode=0755 owner=root
add dir path=usr/share/man group=bin mode=0755 owner=root
add dir path=usr/share/man/man1 group=bin mode=0755 owner=root
add link path=usr/share/man/man1/gtk-config.1 \
	target=../../../sfw/share/man/man1/gtk-config.1
add dir path=usr/share/man/man3 group=bin mode=0755 owner=root
add link path=usr/share/man/man3/gtk.3 target=../../../sfw/share/man/man3/gtk.3
add link path=usr/share/man/man3/libgtk.3 \
	target=../../../sfw/share/man/man3/libgtk.3
end package
