package SUNWwebmin
classification "System/Administration and Configuration"
import SUNWwebminu
import SUNWwebminr
add dir path=usr/bin mode=0755 owner=root group=bin
add link path=usr/bin/webminsetup target=../sfw/bin/webminsetup
add dir path=usr/share mode=0755 owner=root group=sys
add dir path=usr/share/man mode=0755 owner=root group=bin
add dir path=usr/share/man/man1m mode=0755 owner=root group=bin
add link path=usr/share/man/man1m/webmin.1m \
	target=../../../sfw/share/man/man1m/webmin.1m
add link path=usr/share/man/man1m/webminsetup.1m \
	target=../../../sfw/share/man/man1m/webminsetup.1m
version 1.340
end package
