package SUNWGlib
classification "System/Libraries"
import SUNWGlib
version 1.2.10
add dir path=usr/bin group=bin mode=0755 owner=root
add link path=usr/bin/glib-config target=../sfw/bin/glib-config
add dir path=usr/include mode=0755 owner=root group=bin
add link path=usr/include/glib-1.2 target=../sfw/include/glib-1.2
add dir path=usr/lib group=bin mode=0755 owner=root
add link path=usr/lib/libglib-1.2.so.0 target=../sfw/lib/libglib-1.2.so.0
add link path=usr/lib/libglib.so target=../sfw/lib/libglib.so
add link path=usr/lib/libgmodule-1.2.so.0 target=../sfw/lib/libgmodule-1.2.so.0
add link path=usr/lib/libgmodule.so target=../sfw/lib/libgmodule.so
add link path=usr/lib/libgthread-1.2.so.0 target=../sfw/lib/libgthread-1.2.so.0
add link path=usr/lib/libgthread.so target=../sfw/lib/libgthread.so
add dir path=usr/share group=sys mode=0755 owner=root
add dir path=usr/share/man group=bin mode=0755 owner=root
add dir path=usr/share/man/man1 group=bin mode=0755 owner=root
add link path=usr/share/man/man1/glib-config.1 \
	target=../../../sfw/share/man/man1/glib-config.1
add dir path=usr/share/man/man3 group=bin mode=0755 owner=root
add link path=usr/share/man/man3/glib.3 \
	target=../../../sfw/share/man/man3/glib.3
add link path=usr/share/man/man3/libglib.3 \
	target=../../../sfw/share/man/man3/libglib.3
end package
