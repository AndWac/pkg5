package SUNWlibsmbclient
classification "System/Libraries"
summary "A library that permits applications to manipulate CIFS/SMB network resources"
version 3.0.28
add dir path=usr/sfw mode=0755 owner=root group=bin
add dir path=usr/sfw/include mode=0755 owner=root group=bin
add dir path=usr/sfw/lib mode=0755 owner=root group=bin
add dir path=usr/sfw/share mode=0755 owner=root group=bin
add dir path=usr/sfw/share/man mode=0755 owner=root group=bin
add dir path=usr/sfw/share/man/man7 mode=0755 owner=root group=bin
add dir path=usr/sfw/swat mode=0755 owner=root group=bin
add dir path=usr/sfw/swat/help mode=0755 owner=root group=bin
add dir path=usr/sfw/swat/help/manpages mode=0755 owner=root group=bin
add dir path=usr/share mode=0755 owner=root group=sys
add dir path=usr/share/man mode=0755 owner=root group=bin
add dir path=usr/share/man/man7 mode=0755 owner=root group=bin
from SUNWsmbau import
usr/sfw/include/libsmbclient.h
