package library/unixodbc
consolidation "sfw"
classification "Development/Databases"
import SUNWunixodbc
import SUNWunixodbcr
version 2.3.0

chattr etc/odbc/odbc.ini \
    original_name=SUNWunixodbc:etc/odbc/odbc.ini
chattr etc/odbc/odbcinst.ini \
    original_name=SUNWunixodbc:etc/odbc/odbcinst.ini
end package

package SUNWunixodbc
consolidation "sfw"
renamed 133
depend library/unixodbc
version 2.2.14
end package
