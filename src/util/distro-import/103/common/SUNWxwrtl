package SUNWxwrtl
classification "System/X11"
import SUNWxwrtl exclude usr/openwin
# X libraries not yet included
drop usr/lib/$(ARCH64)/libxkbfile.so
drop usr/lib/$(ARCH64)/libxkbfile.so.4
drop usr/lib/libXIE.so
drop usr/lib/libXIE.so.6
drop usr/lib/libXaw.so.4
drop usr/lib/libxkbfile.so
drop usr/lib/libxkbfile.so.4
# Motif
$(sparc_ONLY)drop usr/lib/$(ARCH64)/libMrm.so
$(sparc_ONLY)drop usr/lib/$(ARCH64)/libMrm.so.4
$(sparc_ONLY)drop usr/lib/$(ARCH64)/libUil.so
$(sparc_ONLY)drop usr/lib/$(ARCH64)/libUil.so.3
drop usr/lib/$(ARCH64)/libXm.so
drop usr/lib/$(ARCH64)/libXm.so.4
drop usr/lib/libMrm.so
drop usr/lib/libMrm.so.1.2
drop usr/lib/libMrm.so.3
drop usr/lib/libMrm.so.4
drop usr/lib/libUil.so
drop usr/lib/libUil.so.3
drop usr/lib/libXm.so
drop usr/lib/libXm.so.1.2
drop usr/lib/libXm.so.3
drop usr/lib/libXm.so.4
chattr_glob * edit target openwin X11
#
# the following link was removed in snv_103 but due to bug 2369, we
# continue to instantiate it so it does not get removed on update
#
add link path=usr/lib/64 target=$(ARCH64)
end package
