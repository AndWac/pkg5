package print/cups
consolidation "sfw"
classification "System/Printing"
import SUNWcupsu exclude \
    usr/lib/$(ARCH64)/libcups.so \
    usr/lib/$(ARCH64)/libcups.so.2 \
    usr/lib/$(ARCH64)/libcupsimage.so \
    usr/lib/$(ARCH64)/libcupsimage.so.2 \
    usr/lib/libcups.so \
    usr/lib/libcups.so.2 \
    usr/lib/libcupsimage.so \
    usr/lib/libcupsimage.so.2
end import
import SUNWcupsr
depend print/lp/print-client-commands
version 1.4.5

chattr etc/cups/command.types original_name=SUNWcups:etc/cups/command.types
chattr etc/cups/cupsd.conf original_name=SUNWcups:etc/cups/cupsd.conf
chattr etc/cups/snmp.conf original_name=SUNWcups:etc/cups/snmp.conf
end package

package SUNWcups
consolidation "sfw"
renamed 133
depend print/cups
version 1.4.2
end package
