package SUNWldom
classification "System/Administration and Configuration"
import SUNWldomr.v
import SUNWldomu.v
add driver name=cnex alias=SUNW,sun4v-channel-devices
add driver name=drctl
add driver name=vlds alias=SUNW,sun4v-domain-service
add driver name=vcc alias=SUNW,sun4v-console-concentrator
add driver name=vdc alias=SUNW,sun4v-disk
add driver name=vds alias=SUNW,sun4v-disk-server
add driver name=vldc alias=SUNW,sun4v-channel
add driver name=vnet perms="* 0666 root sys" clone_perms="0666 root sys" \
	alias=SUNW,sun4v-network
add driver name=vsw perms="* 0666 root sys" clone_perms="0666 root sys" \
	alias=SUNW,sun4v-network-switch
end package
