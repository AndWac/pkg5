package SUNWscm
import SUNWscmu
import SUNWscmr
add driver name=nskern perms="* 0666 root sys" privs=sys_devices privs=sys_config
add driver name=ncall perms="* 0666 root sys" privs=sys_devices privs=sys_config
add driver name=nsctl perms="* 0666 root sys" privs=sys_devices privs=sys_config
add driver name=sdbc perms="* 0666 root sys" privs=sys_devices privs=sys_config
end package
