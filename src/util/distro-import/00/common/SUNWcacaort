package SUNWcacaort
import SUNWcacaort
depend SUNWjdmk-base
add dir group=sys mode=0755 owner=root path=etc
add dir group=sys mode=0755 owner=root path=etc/cacao
add dir group=sys mode=0755 owner=root path=etc/cacao/instances
add dir group=sys mode=0755 owner=root path=etc/cacao/instances/default
add dir group=sys mode=0755 owner=root path=etc/cacao/instances/default/modules
end package
