package SUNWbinutils
classification "Development/GNU"
import SUNWbinutils
version 2.15
add link path=usr/bin/gaddr2line target=../sfw/bin/gaddr2line
add link path=usr/bin/gar target=../sfw/bin/gar
add link path=usr/bin/gas target=../sfw/bin/gas
add link path=usr/bin/gc++filt target=../sfw/bin/gc++filt
add link path=usr/bin/ggprof target=../sfw/bin/ggprof
add link path=usr/bin/gld target=../sfw/bin/gld
add link path=usr/bin/gnm target=../sfw/bin/gnm
add link path=usr/bin/gobjcopy target=../sfw/bin/gobjcopy
add link path=usr/bin/gobjdump target=../sfw/bin/gobjdump
add link path=usr/bin/granlib target=../sfw/bin/granlib
add link path=usr/bin/greadelf target=../sfw/bin/greadelf
add link path=usr/bin/gsize target=../sfw/bin/gsize
add link path=usr/bin/gstrings target=../sfw/bin/gstrings
add link path=usr/bin/gstrip target=../sfw/bin/gstrip
add dir path=usr/gnu mode=0755 owner=root group=bin
add dir path=usr/gnu/bin mode=0755 owner=root group=bin
add link path=usr/gnu/bin/addr2line target=../../sfw/bin/gaddr2line
add link path=usr/gnu/bin/ar target=../../sfw/bin/gar
add link path=usr/gnu/bin/as target=../../sfw/bin/gas
add link path=usr/gnu/bin/c++filt target=../../sfw/bin/gc++filt
add link path=usr/gnu/bin/gprof target=../../sfw/bin/ggprof
add link path=usr/gnu/bin/ld target=../../sfw/bin/gld
add link path=usr/gnu/bin/nm target=../../sfw/bin/gnm
add link path=usr/gnu/bin/objcopy target=../../sfw/bin/gobjcopy
add link path=usr/gnu/bin/objdump target=../../sfw/bin/gobjdump
add link path=usr/gnu/bin/ranlib target=../../sfw/bin/granlib
add link path=usr/gnu/bin/readelf target=../../sfw/bin/greadelf
add link path=usr/gnu/bin/size target=../../sfw/bin/gsize
add link path=usr/gnu/bin/strings target=../../sfw/bin/gstrings
add link path=usr/gnu/bin/strip target=../../sfw/bin/gstrip
end package
