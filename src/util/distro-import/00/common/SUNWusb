package SUNWusb
classification "Drivers/Ports"
import SUNWusb
add driver name=hid perms="* 0600 root sys" alias=usbif,class3
add driver name=hubd perms="* 0644 root sys" alias=usbif,class9
add driver name=scsa2usb alias=usbif,class8 alias=usb584,222
add driver name=usb_ia alias=usb,ia
add driver name=usb_mid alias=usb,device
add driver name=usbprn perms="* 0666 root sys" alias=usbif,class7.1
add driver name=usb_ac perms="* 0600 root sys" alias=usbif,class1.1
add driver name=usb_as perms="* 0600 root sys" alias=usbif,class1.2
add driver name=ohci perms="* 0644 root sys" alias=pciclass,0c0310
add driver name=ehci perms="* 0644 root sys" alias=pciclass,0c0320
add driver name=uhci perms="* 0644 root sys" alias=pciclass,0c0300
end package
