package runtime/java
consolidation "ub_javavm"
classification "Development/Java"
import SUNWj6rt
drop usr/jdk/instances/jdk1.6.0/jre/lib/$(ARCH)/motif21/libmawt.so
chattr usr/jdk/instances/jdk1.6.0/jre/lib/fontconfig.OpenSolaris.bfc \
    path=usr/jdk/instances/jdk1.6.0/jre/lib/fontconfig.5.11.bfc
chattr \
    usr/jdk/instances/jdk1.6.0/jre/lib/fontconfig.OpenSolaris.properties.src \
    path=usr/jdk/instances/jdk1.6.0/jre/lib/fontconfig.5.11.properties.src

add link path=usr/bin/ControlPanel target=../java/jre/bin/ControlPanel
add link path=usr/bin/java target=../java/bin/java
add link path=usr/bin/javaws target=../java/bin/javaws
add link path=usr/bin/jcontrol target=../java/jre/bin/jcontrol
add link path=usr/bin/keytool target=../java/bin/keytool
add link path=usr/bin/orbd target=../java/bin/orbd
add link path=usr/bin/policytool target=../java/bin/policytool
add link path=usr/bin/rmid target=../java/bin/rmid
add link path=usr/bin/rmiregistry target=../java/bin/rmiregistry
add link path=usr/bin/servertool target=../java/bin/servertool
add link path=usr/bin/tnameserv target=../java/bin/tnameserv
add link path=usr/java target=jdk/jdk1.6.0_22
add link path=usr/jdk/jdk1.6.0_22 target=instances/jdk1.6.0
add link path=usr/jdk/latest target=jdk1.6.0_22
add link path=usr/share/applications/sun_java.desktop \
    target=../../jdk/instances/jdk1.6.0/jre/lib/desktop/applications/sun_java.desktop
add link path=usr/share/applications/sun-java.desktop \
    target=../../jdk/instances/jdk1.6.0/jre/lib/desktop/applications/sun-java.desktop
add link path=usr/share/applications/sun-javaws.desktop \
    target=../../jdk/instances/jdk1.6.0/jre/lib/desktop/applications/sun-javaws.desktop

