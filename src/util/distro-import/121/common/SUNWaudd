package SUNWaudd
classification "Drivers/Media"
import SUNWaudd
add driver name=audio perms="* 0666 root sys"
add driver name=audioens alias=pci1274,1371 alias=pci1274,5880
add driver name=audiopci alias=pci1274,5000
add driver name=audiots alias=pci10b9,5451
add driver name=audio1575 alias=pci10b9,5455
$(sparc_ONLY)add driver name=audiocs alias=SUNW,CS4231
end package
