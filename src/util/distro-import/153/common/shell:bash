package shell/bash
consolidation "sfw"
classification "System/Shells"
import SUNWbash
import SUNWbashr
version 4.1.9
add file .bashrc \
    path=etc/skel/.bashrc \
    mode=0644 \
    owner=root \
    group=other \
    preserve=true
chattr etc/skel/.bashrc original_name=SUNWcs:etc/skel/.bashrc
end package

package SUNWbash
consolidation "sfw"
renamed 133
depend shell/bash
version 4.0.28
end package
