package SUNWpcmci
classification "System/Hardware"
import SUNWpcmci

add driver name=pcic \
    class=pcmcia \
    alias="pciclass,060500" \
    alias="pciclass,060700"

add driver name=pcs

add driver name=pem \
    perms="* 0666 bin bin" \
    devlink="type=pcmcia:event;name=pem\tpem"

$(sparc_ONLY)add driver name=stp4020 \
$(sparc_ONLY)    class=pcmcia \
$(sparc_ONLY)    alias="SUNW,pcmcia"
end package
