package SUNWpcelx
consolidation "osnet"
classification "Drivers/Networking"
import SUNWpcelx

add driver name=pcelx \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    policy="* read_priv_set=net_rawaccess write_priv_set=net_rawaccess" \
    alias="pccard101,556" \
    alias="pccard101,574" \
    alias="pccard101,589" \
    alias="pccard101,562,0"
end package
