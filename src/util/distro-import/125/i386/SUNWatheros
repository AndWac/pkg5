package SUNWatheros
consolidation "osnet"
classification "Drivers/Networking"
import SUNWatheros

add driver name=ath \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="pci168c,12" \
    alias="pci168c,13" \
    alias="pci168c,1014" \
    alias="pci168c,1a" \
    alias="pci168c,1b" \
    alias="pci168c,23" \
    alias="pciex168c,1c" \
    alias="pciex168c,24"
end package
