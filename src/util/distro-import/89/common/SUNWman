package SUNWman
import SUNWman exclude usr/share/man/man1/ex.1 usr/share/man/man1/sh.1 usr/share/man/man1/vi.1 usr/share/man/man1/view.1
drop usr/share/man/man.cf
from ./SUNWfixes import
usr/share/man/man.cf
end import
# Use the 32-bit ksh93 as a replacement /bin/sh
drop usr/share/man/man1/ksh.1
drop usr/share/man/man1/rksh.1
add link path=usr/share/man/man1/ksh.1 target=ksh93.1
add link path=usr/share/man/man1/rksh.1 target=ksh93.1
add link path=usr/share/man/man1/sh.1 target=ksh93.1
end package
