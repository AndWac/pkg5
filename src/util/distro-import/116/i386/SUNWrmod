package SUNWrmod
classification "System/Core"
import SUNWrmodr
chattr boot/solaris/bootenv.rc original_name=SUNWrmodr:boot/solaris/bootenv.rc
drop boot/solaris.xpm.gz
add file solaris.xpm path=boot/solaris.xpm mode=0644 owner=root group=sys
add file master path=boot/solaris/devicedb/master mode=0644 owner=root \
	group=sys preserve=true
import SUNWrmodu
end package
