package web/php-52/extension/php-mysql
consolidation "sfw"
classification "Development/PHP"
import SUNWphp52r-mysql
import SUNWphp52u-mysql
depend web/php-52
version 5.2.17

chattr etc/php/5.2/conf.d/mysql.ini \
	original_name=SUNWphp52-mysql:etc/php/5.2/conf.d/mysql.ini
chattr etc/php/5.2/conf.d/mysqli.ini \
	original_name=SUNWphp52-mysql:etc/php/5.2/conf.d/mysqli.ini
chattr etc/php/5.2/conf.d/pdo_mysql.ini \
	original_name=SUNWphp52-mysql:etc/php/5.2/conf.d/pdo_mysql.ini
chattr etc/php/5.2/zts-conf.d/mysql.ini \
	original_name=SUNWphp52-mysql:etc/php/5.2/zts-conf.d/mysql.ini
chattr etc/php/5.2/zts-conf.d/mysqli.ini \
	original_name=SUNWphp52-mysql:etc/php/5.2/zts-conf.d/mysqli.ini
chattr etc/php/5.2/zts-conf.d/pdo_mysql.ini \
	original_name=SUNWphp52-mysql:etc/php/5.2/zts-conf.d/pdo_mysql.ini
end package

package SUNWphp52-mysql
consolidation "sfw"
renamed 133
depend web/php-52/extension/php-mysql
version 5.2.12
end package
