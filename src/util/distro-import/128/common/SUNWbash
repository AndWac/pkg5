package SUNWbash
consolidation "sfw"
classification "System/Shells"
import SUNWbash
import SUNWbashr
version 4.0.28
add file .bashrc \
    path=etc/skel/.bashrc \
    mode=0644 \
    owner=root \
    group=other \
    preserve=true
chattr etc/skel/.bashrc original_name=SUNWcs:etc/skel/.bashrc
end package
