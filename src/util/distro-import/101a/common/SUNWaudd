package SUNWaudd
classification "Drivers/Media"
import SUNWaudd
$(sparc_ONLY)add driver name=audiocs alias=SUNW,CS4231
$(sparc_ONLY)add driver name=audiots alias=pci10b9,5451
$(sparc_ONLY)add driver name=audio1575 alias=pci10b9,5455
# BEGIN NON-REDISTRIBUTABLE
# add driver name=audioens alias=pci1274,1371 alias=pci1274,5880
drop kernel/drv/$(ARCH64)/audioens
drop kernel/drv/audioens.conf
$(i386_ONLY)drop kernel/drv/audioens
# END NON-REDISTRIBUTABLE
end package
