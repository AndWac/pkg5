package SUNWgnome-display-mgr
classification "Desktop (GNOME)/Sessions"
import SUNWgnome-display-mgr
import SUNWgnome-display-mgr-root
drop var/svc/manifest/application/graphical-login/gdm.xml
add file gdm.xml path=var/svc/manifest/application/graphical-login/gdm.xml \
	mode=0444 owner=root group=sys \
	restart_fmri=svc:/system/manifest-import:default
end package
