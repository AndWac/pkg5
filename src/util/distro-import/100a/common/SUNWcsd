package SUNWcsd
classification "System/Core"
import SUNWcsd
drop etc/driver_aliases
drop etc/name_to_major
add file driver_aliases path=etc/driver_aliases \
    mode=0644 owner=root group=sys opensolaris.zone=global
add file name_to_major path=etc/name_to_major \
    mode=0644 owner=root group=sys opensolaris.zone=global
end package
