package driver/network/ral
consolidation "osnet"
classification "Drivers/Networking"
import SUNWralink

add driver name=ral \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="pci1814,201"
end package

package SUNWralink
consolidation "osnet"
renamed 133
depend driver/network/ral
end package
