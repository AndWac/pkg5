package driver/network/iwp
consolidation "osnet"
classification "Drivers/Networking"
import SUNWiwp

add driver name=iwp \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="pciex8086,422b" \
    alias="pciex8086,422c" \
    alias="pciex8086,4238" \
    alias="pciex8086,4239" \
    alias="pciex8086,008d" \
    alias="pciex8086,008e"
end package

package SUNWiwp
consolidation "osnet"
renamed 133
depend driver/network/iwp
end package
