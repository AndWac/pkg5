package driver/network/atu
consolidation "osnet"
classification "Drivers/Networking"
import SUNWatu

add driver name=atu \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="usb1915,2233"
end package

package SUNWatu
consolidation "osnet"
renamed 133
depend driver/network/atu
end package
