package system/xvm/xvmstore
consolidation "xvm"
classification "System/Virtualization"
import SUNWxvmdomu
import SUNWxvmdomr exclude var var/log var/log/xen
add file xvm-vnc-config path=lib/svc/method/xvm-vnc-config mode=0555 \
	owner=root group=bin
add file vnc-config.xml path=var/svc/manifest/system/xvm/vnc-config.xml \
	mode=0444 owner=root group=sys \
	restart_fmri=svc:/system/manifest-import:default
version 3.4.2
end package

