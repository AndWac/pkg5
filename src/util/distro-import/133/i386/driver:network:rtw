package driver/network/rtw
consolidation "osnet"
classification "Drivers/Networking"
import SUNWrtw

add driver name=rtw \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="pci10ec,8180"
end package

package SUNWrtw
consolidation "osnet"
renamed 133
depend driver/network/rtw
end package
