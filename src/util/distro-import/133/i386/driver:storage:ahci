package driver/storage/ahci
consolidation "osnet"
classification "Drivers/Ports"
import SUNWahci

add driver name=ahci \
    perms="* 0644 root sys" \
    class=scsi-self-identifying \
    alias="pciclass,010601"
end package

package SUNWahci
consolidation "osnet"
renamed 133
depend driver/storage/ahci
end package
