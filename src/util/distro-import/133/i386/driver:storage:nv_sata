package driver/storage/nv_sata
consolidation "osnet"
classification "Drivers/Ports"
import SUNWnvsata

add driver name=nv_sata \
    perms="* 0644 root sys" \
    class=scsi-self-identifying \
    alias="pci10de,37f" \
    alias="pci10de,55" \
    alias="pci10de,54" \
    alias="pci10de,266" \
    alias="pci10de,267"
end package

package SUNWnvsata
consolidation "osnet"
renamed 133
depend driver/storage/nv_sata
end package
