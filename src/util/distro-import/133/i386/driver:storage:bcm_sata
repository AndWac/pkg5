package driver/storage/bcm_sata
consolidation "osnet"
classification "Drivers/Storage"
import SUNWbcmsata

add driver name=bcm_sata \
    perms="* 0644 root sys" \
    class=scsi-self-identifying \
    alias="pci1166,24a"
end package

package SUNWbcmsata
consolidation "osnet"
renamed 133
depend driver/storage/bcm_sata
end package
