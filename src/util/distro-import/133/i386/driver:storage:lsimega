package driver/storage/lsimega
consolidation "osnet"
classification "Drivers/Storage"
import SUNWlsimega

add driver name=lsimega \
    class=scsi \
    alias="pci1028,13" \
    alias="pci1000,407" \
    alias="pci1000,407.1000.532" \
    alias="pci1000,408" \
    alias="pci1000,408.1000.2" \
    alias="pci1000,409" \
    alias="pci1000,1960"
end package

package SUNWlsimega
consolidation "osnet"
renamed 133
depend driver/storage/lsimega
end package
