package driver/audio/audioixp
consolidation "osnet"
classification "Drivers/Media"
import SUNWadixp

add driver name=audioixp \
    alias="pci1002,4370" \
    alias="pci1002,4361" \
    alias="pci1002,4341"
end package

package SUNWadixp
consolidation "osnet"
renamed 133
depend driver/audio/audioixp
end package
