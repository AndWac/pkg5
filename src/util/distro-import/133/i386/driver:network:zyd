package driver/network/zyd
consolidation "osnet"
classification "Drivers/Networking"
import SUNWzyd

add driver name=zyd \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="usbace,1211" \
    alias="usb13b1,24"
end package

package SUNWzyd
consolidation "osnet"
renamed 133
depend driver/network/zyd
end package
