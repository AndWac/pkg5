package driver/network/arn
consolidation "osnet"
classification "Drivers/Networking"
import SUNWarn

add driver name=arn \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="pciex168c,2a" \
    alias="pciex168c,2b"
end package

package SUNWarn
consolidation "osnet"
renamed 133
depend driver/network/arn
end package
