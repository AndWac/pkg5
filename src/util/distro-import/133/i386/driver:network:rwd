package driver/network/rwd
consolidation "osnet"
classification "Drivers/Networking"
import SUNWrwd

add driver name=rwd \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="pci1814,401"
end package

package SUNWrwd
consolidation "osnet"
renamed 133
depend driver/network/rwd
end package
