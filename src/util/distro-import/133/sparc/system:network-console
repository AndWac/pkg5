package system/network-console
consolidation "osnet"
classification "System/Core"
import SUNWcvc.u
import SUNWcvcr.u

add driver name=cvc \
    perms="* 0600 root sys"

add driver name=cvcredir \
    perms="* 0600 root sys"
end package

package SUNWcvc
consolidation "osnet"
renamed 133
depend system/network-console
end package
