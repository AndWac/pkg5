package driver/storage/ssd
consolidation "osnet"
classification "Drivers/Storage"
import SUNWssad

add driver name=ssd \
    perms="* 0640 root sys" \
    alias="SUNW,ssd" \
    alias="scsa,00.bfcp" \
    alias="scsa,00.bvhci"
end package

package SUNWssad
consolidation "osnet"
renamed 133
depend driver/storage/ssd
end package
