package system/file-system/nfs
consolidation "osnet"
classification "System/File System"
import SUNWnfscu
import SUNWnfscr
import SUNWnfsckr
add group groupname=unknown gid=96
add user username=unknown uid=96 group=unknown gcos-field="Unknown Remote UID"
depend system/kernel/secure-rpc

chattr etc/default/nfs \
	original_name=SUNWnfsc:etc/default/nfs
chattr etc/nfssec.conf \
	original_name=SUNWnfsc:etc/nfssec.conf
end package

package SUNWnfsc
consolidation "osnet"
renamed 133
depend system/file-system/nfs
end package
