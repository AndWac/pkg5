package driver/serial/usbsacm
consolidation "osnet"
classification "Drivers/Ports"
import SUNWuacm

add driver name=usbsacm \
    perms="* 0666 root sys" \
    alias="usb,class2.2.0" \
    alias="usbif,class2.2" \
    alias="usbc88,17da" \
    alias="usb1410,1110"
end package

package SUNWuacm
consolidation "osnet"
renamed 133
depend driver/serial/usbsacm
end package
