package service/storage/avs/cache-management
consolidation "osnet"
classification "System/Enterprise Management"
import SUNWscmu
import SUNWscmr

add driver name=ncall \
    perms="* 0666 root sys" \
    privs=sys_devices \
    privs=sys_config \
    devlink="type=ddi_pseudo;name=ncall\t\D"

add driver name=nsctl \
    perms="* 0666 root sys" \
    privs=sys_devices \
    privs=sys_config \
    devlink="type=ddi_pseudo;name=nsctl\t\D"

add driver name=nskern \
    perms="* 0666 root sys" \
    privs=sys_devices \
    privs=sys_config \
    devlink="type=ddi_pseudo;name=nskern\t\D"

add driver name=sdbc \
    perms="* 0666 root sys" \
    privs=sys_devices \
    privs=sys_config \
    devlink="type=ddi_pseudo;name=sdbc\t\D"

end package

package SUNWscm
consolidation "osnet"
renamed 133
depend service/storage/avs/cache-management
end package
