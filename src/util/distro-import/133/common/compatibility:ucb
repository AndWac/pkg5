package compatibility/ucb
consolidation "osnet"
classification "System/Core"
import SUNWscpr
import SUNWscpu exclude usr/ucb/e usr/ucb/ex usr/ucb/vi usr/ucb/view
add link path=usr/ucb/e target=../bin/edit
add link path=usr/ucb/ex target=../bin/edit
add link path=usr/ucb/vi target=../bin/edit
add link path=usr/ucb/view target=../bin/edit

chattr etc/mail/Mail.rc \
	original_name=SUNWscp:etc/mail/Mail.rc
end package

package SUNWscp
consolidation "osnet"
renamed 133
depend compatibility/ucb
end package
