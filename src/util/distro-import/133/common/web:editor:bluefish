package web/editor/bluefish
consolidation "gnome"
classification "Applications/Internet"
classification "Development/Editors"
import SUNWbluefish

chattr usr/share/mime/packages/bluefish.xml \
	original_name=SUNWbluefish:usr/share/mime/packages/bluefish.xml
end package

package SUNWbluefish
consolidation "gnome"
renamed 133
depend web/editor/bluefish
end package
