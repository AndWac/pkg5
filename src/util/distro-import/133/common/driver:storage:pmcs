package driver/storage/pmcs
consolidation "osnet"
classification "Drivers/Storage"
import SUNWpmcsr
import SUNWpmcsu

add driver name=pmcs \
    perms="* 0600 root sys" \
    class=scsi-self-identifying \
    alias="pciex11f8,8001"

chattr kernel/drv/pmcs.conf \
	original_name=SUNWpmcs:kernel/drv/pmcs.conf
end package

package SUNWpmcs
consolidation "osnet"
renamed 133
depend driver/storage/pmcs
end package
