package system/file-system/autofs
consolidation "osnet"
classification "System/File System"
import SUNWatfsu
import SUNWatfsr

chattr etc/auto_home \
	original_name=SUNWatfs:etc/auto_home
chattr etc/auto_master \
	original_name=SUNWatfs:etc/auto_master
chattr etc/default/autofs \
	original_name=SUNWatfs:etc/default/autofs
end package

package SUNWatfs
consolidation "osnet"
renamed 133
depend system/file-system/autofs
end package
