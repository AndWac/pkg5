package driver/network/nxge
consolidation "osnet"
classification "Drivers/Networking"
$(i386_ONLY)import SUNWnxge
$(sparc_ONLY)import SUNWnxge.u
$(sparc_ONLY)import SUNWnxge.v

$(i386_ONLY)add driver name=nxge \
$(i386_ONLY)    perms="* 0600 root sys" \
$(i386_ONLY)    alias="pciex108e,abcd" \
$(i386_ONLY)    alias="SUNW,niusl"

$(sparc_ONLY)add driver name=nxge \
$(sparc_ONLY)    perms="* 0600 root sys" \
$(sparc_ONLY)    alias="pciex108e,abcd" \
$(sparc_ONLY)    alias="SUNW,niusl" \
$(sparc_ONLY)    alias="SUNW,rock-pciex108e,abcd" \
$(sparc_ONLY)    alias="SUNW,niusl-kt"


$(i386_ONLY)chattr kernel/drv/nxge.conf \
$(i386_ONLY)	original_name=SUNWnxge:kernel/drv/nxge.conf
$(sparc_ONLY)chattr platform/sun4u/kernel/drv/nxge.conf \
$(sparc_ONLY)	original_name=SUNWnxge:platform/sun4u/kernel/drv/nxge.conf
$(sparc_ONLY)chattr platform/sun4v/kernel/drv/nxge.conf \
$(sparc_ONLY)	original_name=SUNWnxge:platform/sun4v/kernel/drv/nxge.conf
end package

package SUNWnxge
consolidation "osnet"
renamed 133
depend driver/network/nxge
end package
