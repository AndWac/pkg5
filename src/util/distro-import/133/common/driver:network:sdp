package driver/network/sdp
consolidation "osnet"
classification "System/Hardware"
import SUNWibsdp

add driver name=sdp \
    perms="* 0644 root sys" \
    perms="sdp 0666 root sys"

chattr etc/sdp.conf \
	original_name=SUNWibsdp:etc/sdp.conf
end package

package SUNWibsdp
consolidation "osnet"
renamed 133
depend driver/network/sdp
end package
