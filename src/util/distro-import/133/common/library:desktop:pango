package library/desktop/pango
consolidation "gnome"
classification "Desktop (GNOME)/Libraries"
import SUNWpango
import SUNWpango-devel
import SUNWpango-root

$(i386_ONLY)chattr etc/amd64/pango/pango.modules \
$(i386_ONLY)	original_name=SUNWpango:etc/amd64/pango/pango.modules
$(i386_ONLY)chattr etc/amd64/pango/pangox.aliases \
$(i386_ONLY)	original_name=SUNWpango:etc/amd64/pango/pangox.aliases
$(sparc_ONLY)chattr etc/sparcv9/pango/pango.modules \
$(sparc_ONLY)	original_name=SUNWpango:etc/sparcv9/pango/pango.modules
$(sparc_ONLY)chattr etc/sparcv9/pango/pangox.aliases \
$(sparc_ONLY)	original_name=SUNWpango:etc/sparcv9/pango/pangox.aliases
chattr etc/pango/pango.modules \
	original_name=SUNWpango:etc/pango/pango.modules
chattr etc/pango/pangox.aliases \
	original_name=SUNWpango:etc/pango/pangox.aliases
end package

package SUNWpango
consolidation "gnome"
renamed 133
depend library/desktop/pango
end package
