package system/library/storage/fibre-channel/hbaapi
consolidation "osnet"
classification "System/Libraries"
import SUNWcfcl
import SUNWcfclr
add file hba.conf path=etc/hba.conf mode=0644 owner=root group=bin \
	preserve=true
depend system/library/storage/fibre-channel/libsun_fc

chattr etc/hba.conf \
	original_name=SUNWcfcl:etc/hba.conf
end package

package SUNWcfcl
consolidation "osnet"
renamed 133
depend system/library/storage/fibre-channel/hbaapi
end package
