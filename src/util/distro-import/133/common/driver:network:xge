package driver/network/xge
consolidation "osnet"
classification "Drivers/Networking"
import SUNWxge

add driver name=xge \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="pci17d5,5731" \
    alias="pci17d5,5831" \
    alias="pci17d5,5832"
end package

package SUNWxge
consolidation "osnet"
renamed 133
depend driver/network/xge
end package
