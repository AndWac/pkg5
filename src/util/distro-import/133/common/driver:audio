package driver/audio
consolidation "osnet"
classification "Drivers/Media"
import SUNWaudd

add driver name=audio \
    perms="* 0666 root sys"

add driver name=audio1575 \
    alias="pci10b9,5455"

$(sparc_ONLY)add driver name=audiocs \
$(sparc_ONLY)    alias="SUNW,CS4231"

add driver name=audioens \
    alias="pci1274,1371" \
    alias="pci1274,5880"

add driver name=audiopci \
    alias="pci1274,5000"

add driver name=audiots \
    alias="pci10b9,5451"
end package

package SUNWaudd
consolidation "osnet"
renamed 133
depend driver/audio
end package
