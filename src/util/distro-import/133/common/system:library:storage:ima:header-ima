package system/library/storage/ima/header-ima
consolidation "osnet"
classification "System/Libraries"
import SUNWimac
import SUNWimacr
add file ima.conf path=etc/ima.conf mode=0644 owner=root group=bin \
	preserve=true
depend system/library/storage/ima

chattr etc/ima.conf \
	original_name=SUNWimac:etc/ima.conf
end package

package SUNWimac
consolidation "osnet"
renamed 133
depend system/library/storage/ima/header-ima
end package
