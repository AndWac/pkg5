package database/mysql-51
consolidation "sfw"
classification "System/Databases"
import SUNWmysql51r
import SUNWmysql51u
version 5.1.37
depend database/mysql-common

chattr etc/mysql/5.1/my.cnf \
	original_name=SUNWmysql51:etc/mysql/5.1/my.cnf
end package

package SUNWmysql51
consolidation "sfw"
renamed 133
depend database/mysql-51
version 5.1.37
end package
