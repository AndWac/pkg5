package driver/network/ibd
consolidation "osnet"
classification "System/Hardware"
import SUNWipoib

add driver name=ibd \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="ib.ipib"

chattr kernel/drv/ibd.conf \
	original_name=SUNWipoib:kernel/drv/ibd.conf
end package

package SUNWipoib
consolidation "osnet"
renamed 133
depend driver/network/ibd
end package
