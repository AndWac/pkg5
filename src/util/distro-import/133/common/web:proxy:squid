package web/proxy/squid
consolidation "sfw"
classification "Web Services/Application and Web Servers"
import SUNWsquidu
import SUNWsquidr
version 2.7.6

chattr etc/squid/cachemgr.conf \
	original_name=SUNWsquid:etc/squid/cachemgr.conf
chattr etc/squid/mime.conf \
	original_name=SUNWsquid:etc/squid/mime.conf
chattr etc/squid/msntauth.conf \
	original_name=SUNWsquid:etc/squid/msntauth.conf
chattr etc/squid/squid.conf \
	original_name=SUNWsquid:etc/squid/squid.conf
end package

package SUNWsquid
consolidation "sfw"
renamed 133
depend web/proxy/squid
version 2.7.6
end package
