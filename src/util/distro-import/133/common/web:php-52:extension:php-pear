package web/php-52/extension/php-pear
consolidation "sfw"
classification "Development/PHP"
import SUNWphp52r-pear
import SUNWphp52u-pear
depend web/php-52
version 5.2.12

chattr etc/php/5.2/pear.conf \
	original_name=SUNWphp52-pear:etc/php/5.2/pear.conf
chattr var/php/5.2/pear/.channels/__uri.reg \
	original_name=SUNWphp52-pear:var/php/5.2/pear/.channels/__uri.reg
chattr var/php/5.2/pear/.channels/doc.php.net.reg \
	original_name=SUNWphp52-pear:var/php/5.2/pear/.channels/doc.php.net.reg
chattr var/php/5.2/pear/.channels/pear.php.net.reg \
	original_name=SUNWphp52-pear:var/php/5.2/pear/.channels/pear.php.net.reg
chattr var/php/5.2/pear/.channels/pecl.php.net.reg \
	original_name=SUNWphp52-pear:var/php/5.2/pear/.channels/pecl.php.net.reg
end package

package SUNWphp52-pear
consolidation "sfw"
renamed 133
depend web/php-52/extension/php-pear
version 5.2.12
end package
