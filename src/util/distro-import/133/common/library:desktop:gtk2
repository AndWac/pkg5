package library/desktop/gtk2
consolidation "gnome"
classification "Desktop (GNOME)/Libraries"
import SUNWgtk2
import SUNWgtk2-devel
import SUNWgtk2-root

$(i386_ONLY)chattr etc/amd64/gtk-2.0/gdk-pixbuf.loaders \
$(i386_ONLY)	original_name=SUNWgtk2:etc/amd64/gtk-2.0/gdk-pixbuf.loaders
$(i386_ONLY)chattr etc/amd64/gtk-2.0/gtk.immodules \
$(i386_ONLY)	original_name=SUNWgtk2:etc/amd64/gtk-2.0/gtk.immodules
$(sparc_ONLY)chattr etc/sparcv9/gtk-2.0/gdk-pixbuf.loaders \
$(sparc_ONLY)	original_name=SUNWgtk2:etc/sparcv9/gtk-2.0/gdk-pixbuf.loaders
$(sparc_ONLY)chattr etc/sparcv9/gtk-2.0/gtk.immodules \
$(sparc_ONLY)	original_name=SUNWgtk2:etc/sparcv9/gtk-2.0/gtk.immodules
chattr etc/gtk-2.0/gdk-pixbuf.loaders \
	original_name=SUNWgtk2:etc/gtk-2.0/gdk-pixbuf.loaders
chattr etc/gtk-2.0/gtk.immodules \
	original_name=SUNWgtk2:etc/gtk-2.0/gtk.immodules
end package

package SUNWgtk2
consolidation "gnome"
renamed 133
depend library/desktop/gtk2
end package
