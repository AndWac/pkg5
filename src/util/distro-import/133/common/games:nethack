package games/nethack
consolidation "sfw"
classification "Applications/Games"
import SUNWnethack
import SUNWnethackr
version 3.4.3

chattr var/games/nethack/logfile \
	original_name=SUNWnethack:var/games/nethack/logfile
chattr var/games/nethack/perm \
	original_name=SUNWnethack:var/games/nethack/perm
chattr var/games/nethack/record \
	original_name=SUNWnethack:var/games/nethack/record
end package

package SUNWnethack
consolidation "sfw"
renamed 133
depend games/nethack
version 3.4.3
end package
