package driver/storage/sv
consolidation "osnet"
classification "Drivers/Storage"
import SUNWspsvu
import SUNWspsvr

add driver name=sv \
    perms="* 0666 root sys" \
    privs=sys_devices \
    privs=sys_config \
    devlink="type=ddi_pseudo;name=sv\t\D"

end package

package SUNWspsv
consolidation "osnet"
renamed 133
depend driver/storage/sv
end package
