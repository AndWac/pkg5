package driver/network/llc2
consolidation "osnet"
classification "Drivers/Networking"
import SUNWllc
# BEGIN NON-REDISTRIBUTABLE
drop usr/include/sys/llc2.h
# END NON-REDISTRIBUTABLE
import SUNWllcr

add driver name=llc2 \
    perms="* 0666 root sys"

chattr etc/init.d/llc2 \
	original_name=SUNWllc:etc/init.d/llc2
end package

package SUNWllc
consolidation "osnet"
renamed 133
depend driver/network/llc2
end package
