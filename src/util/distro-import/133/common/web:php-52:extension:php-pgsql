package web/php-52/extension/php-pgsql
consolidation "sfw"
classification "Development/PHP"
import SUNWphp52r-pgsql
import SUNWphp52u-pgsql
depend web/php-52
version 5.2.12

chattr etc/php/5.2/conf.d/pdo_pgsql.ini \
	original_name=SUNWphp52-pgsql:etc/php/5.2/conf.d/pdo_pgsql.ini
chattr etc/php/5.2/conf.d/pgsql.ini \
	original_name=SUNWphp52-pgsql:etc/php/5.2/conf.d/pgsql.ini
chattr etc/php/5.2/zts-conf.d/pdo_pgsql.ini \
	original_name=SUNWphp52-pgsql:etc/php/5.2/zts-conf.d/pdo_pgsql.ini
chattr etc/php/5.2/zts-conf.d/pgsql.ini \
	original_name=SUNWphp52-pgsql:etc/php/5.2/zts-conf.d/pgsql.ini
end package

package SUNWphp52-pgsql
consolidation "sfw"
renamed 133
depend web/php-52/extension/php-pgsql
version 5.2.12
end package
