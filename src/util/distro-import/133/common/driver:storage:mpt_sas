package driver/storage/mpt_sas
consolidation "osnet"
classification "Drivers/Storage"
import SUNWmptsas

add driver name=mpt_sas \
    class=scsi-self-identifying \
    alias="pci1000,72" \
    alias="pciex1000,72"

chattr kernel/drv/mpt_sas.conf \
	original_name=SUNWmptsas:kernel/drv/mpt_sas.conf
end package

package SUNWmptsas
consolidation "osnet"
renamed 133
depend driver/storage/mpt_sas
end package
