package web/browser/firefox
consolidation "gnome"
classification "Applications/Internet"
import SUNWfirefox
import SUNWfirefox-devel

chattr usr/lib/firefox/.autoreg \
	original_name=SUNWfirefox:usr/lib/firefox/.autoreg
end package

package SUNWfirefox
consolidation "gnome"
renamed 133
depend web/browser/firefox
end package
