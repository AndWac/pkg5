package service/security/gss
consolidation "osnet"
classification "System/Security"
import SUNWgssc

chattr etc/gss/gsscred.conf \
	original_name=SUNWgssc:etc/gss/gsscred.conf
chattr etc/gss/mech \
	original_name=SUNWgssc:etc/gss/mech
chattr etc/gss/qop \
	original_name=SUNWgssc:etc/gss/qop
end package

package SUNWgssc
consolidation "osnet"
renamed 133
depend service/security/gss
end package
