package driver/network/hxge
consolidation "osnet"
classification "Drivers/Networking"
import SUNWhxge

$(i386_ONLY)add driver name=hxge \
$(i386_ONLY)    perms="* 0600 root sys" \
$(i386_ONLY)    alias="pci108e,aaaa"

$(sparc_ONLY)add driver name=hxge \
$(sparc_ONLY)    perms="* 0600 root sys" \
$(sparc_ONLY)    alias="pciex108e,aaaa"

chattr kernel/drv/hxge.conf \
	original_name=SUNWhxge:kernel/drv/hxge.conf
end package

package SUNWhxge
consolidation "osnet"
renamed 133
depend driver/network/hxge
end package
