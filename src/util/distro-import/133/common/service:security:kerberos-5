package service/security/kerberos-5
consolidation "osnet"
classification "System/Security"
import SUNWkrbu
import SUNWkrbr
depend service/security/gss

chattr etc/krb5/krb5.conf \
	original_name=SUNWkrb:etc/krb5/krb5.conf
chattr etc/krb5/warn.conf \
	original_name=SUNWkrb:etc/krb5/warn.conf
end package

package SUNWkrb
consolidation "osnet"
renamed 133
depend service/security/kerberos-5
end package
