package database/mysql-common
consolidation "sfw"
classification "System/Databases"
summary "MySQL Database Management System (Base)"
add group groupname=mysql gid=70
add user username=mysql password=NP uid=70 group=mysql gcos-field="MySQL Reserved UID" ftpuser=false
end package

package SUNWmysql-base
consolidation "sfw"
renamed 133
depend database/mysql-common
end package
