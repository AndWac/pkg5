package data/docbook
consolidation "gnome"
classification "Desktop (GNOME)/Documentation"
import SUNWgnome-xml-share
import SUNWgnome-xml-root
depend_path /usr/bin/xmlcatalog

chattr etc/sgml/catalog \
	original_name=SUNWgnome-xml:etc/sgml/catalog
chattr etc/sgml/sgml.conf \
	original_name=SUNWgnome-xml:etc/sgml/sgml.conf
chattr etc/xml/catalog \
	original_name=SUNWgnome-xml:etc/xml/catalog
chattr etc/xml/docbook-xmlcatalog \
	original_name=SUNWgnome-xml:etc/xml/docbook-xmlcatalog
end package

package SUNWgnome-xml
consolidation "gnome"
renamed 133
depend data/docbook
end package
