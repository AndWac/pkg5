package print/lp/ipp/ipp-listener
consolidation "osnet"
classification "System/Printing"
import SUNWipplu
import SUNWipplr

chattr etc/apache/httpd-standalone-ipp.conf \
	original_name=SUNWippl:etc/apache/httpd-standalone-ipp.conf
end package

package SUNWippl
consolidation "osnet"
renamed 133
depend print/lp/ipp/ipp-listener
end package
