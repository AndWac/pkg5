package system/kernel/cpu-counters
consolidation "osnet"
classification "System/Core"
$(i386_ONLY)import SUNWcpc.i
$(sparc_ONLY)import SUNWcpc.u
$(sparc_ONLY)import SUNWcpc.v

add driver name=cpc \
    perms="shared 0666 root sys"
end package

package SUNWcpc
consolidation "osnet"
renamed 133
depend system/kernel/cpu-counters
end package
