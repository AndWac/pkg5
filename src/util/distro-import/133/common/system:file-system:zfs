package system/file-system/zfs
consolidation "osnet"
classification "System/File System"
import SUNWzfsu
import SUNWzfsr
import SUNWzfskr

add driver name=zfs \
    perms="* 0600 root sys" \
    perms="zfs 0666 root sys"
end package

package SUNWzfs
consolidation "osnet"
renamed 133
depend system/file-system/zfs
end package
