package system/font/truetype/bh-luxi
consolidation "X"
classification "System/Fonts"
import SUNWttf-bh-luxi
add dir path=etc mode=0755 owner=root group=sys
add dir path=etc/X11 mode=0755 owner=root group=sys
add dir path=etc/X11/fontpath.d mode=0755 owner=root group=sys
add link path=etc/X11/fontpath.d/bh-luxi:pri=42 \
    target=../../../usr/share/fonts/TrueType/bh-luxi
end package

package SUNWttf-bh-luxi
consolidation "X"
renamed 133
depend system/font/truetype/bh-luxi
end package
