package system/accounting/legacy
consolidation "osnet"
classification "System/Administration and Configuration"
import SUNWaccu
import SUNWaccr

chattr etc/acct/holidays \
	original_name=SUNWacc:etc/acct/holidays
chattr etc/init.d/acct \
	original_name=SUNWacc:etc/init.d/acct
chattr var/spool/cron/crontabs/sys \
	original_name=SUNWacc:var/spool/cron/crontabs/sys
end package

package SUNWacc
consolidation "osnet"
renamed 133
depend system/accounting/legacy
end package
