package driver/network/ixgb
consolidation "osnet"
classification "Drivers/Networking"
import SUNWixgb

add driver name=ixgb \
    perms="* 0666 root root" \
    alias="pci8086,1a48" \
    alias="pci8086,1048" \
    alias="pci8086,a11f" \
    alias="pci8086,109e"
end package

package SUNWixgb
consolidation "osnet"
renamed 133
depend driver/network/ixgb
end package
