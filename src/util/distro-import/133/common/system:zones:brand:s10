package system/zones/brand/s10
consolidation "osnet"
classification "System/Virtualization"
import SUNWs10brandr
import SUNWs10brandu

chattr etc/brand/solaris10/pkgrm.conf \
	original_name=SUNWs10brand:etc/brand/solaris10/pkgrm.conf
chattr etc/brand/solaris10/smf_disable.conf \
	original_name=SUNWs10brand:etc/brand/solaris10/smf_disable.conf
end package

package SUNWs10brand
consolidation "osnet"
renamed 133
depend system/zones/brand/s10
end package
