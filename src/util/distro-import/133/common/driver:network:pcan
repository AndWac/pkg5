package driver/network/pcan
consolidation "osnet"
classification "Drivers/Networking"
import SUNWpcan

add driver name=pcan \
    perms="* 0666 root sys" \
    clone_perms="0666 root sys" \
    alias="pccard15f,a" \
    alias="pccard15f,7" \
    alias="pci14b9,5000" \
    alias="pci14b9,a504"
end package

package SUNWpcan
consolidation "osnet"
renamed 133
depend driver/network/pcan
end package
