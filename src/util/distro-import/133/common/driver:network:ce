package driver/network/ce
consolidation "nspg"
classification "Drivers/Networking"
import SUNWced
import SUNWcedu

add driver name=ce \
    perms="* 0600 root sys" \
    alias="pci108e,abba" \
    alias="pci100b,35"
end package

package SUNWced
consolidation "nspg"
renamed 133
depend driver/network/ce
end package
