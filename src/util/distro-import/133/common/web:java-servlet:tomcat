package web/java-servlet/tomcat
consolidation "sfw"
classification "Web Services/Application and Web Servers"
import SUNWtcatu
import SUNWtcatr
version 6.0.24

chattr var/tomcat6/conf/catalina.policy \
	original_name=SUNWtcat:var/tomcat6/conf/catalina.policy
chattr var/tomcat6/conf/catalina.properties \
	original_name=SUNWtcat:var/tomcat6/conf/catalina.properties
chattr var/tomcat6/conf/context.xml \
	original_name=SUNWtcat:var/tomcat6/conf/context.xml
chattr var/tomcat6/conf/logging.properties \
	original_name=SUNWtcat:var/tomcat6/conf/logging.properties
chattr var/tomcat6/conf/server.xml \
	original_name=SUNWtcat:var/tomcat6/conf/server.xml
chattr var/tomcat6/conf/tomcat-users.xml \
	original_name=SUNWtcat:var/tomcat6/conf/tomcat-users.xml
chattr var/tomcat6/conf/web.xml \
	original_name=SUNWtcat:var/tomcat6/conf/web.xml
chattr var/tomcat6/webapps/ROOT/index.jsp \
	original_name=SUNWtcat:var/tomcat6/webapps/ROOT/index.jsp
end package

package SUNWtcat
consolidation "sfw"
renamed 133
depend web/java-servlet/tomcat
version 6.0.20
end package
