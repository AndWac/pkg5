package network/iscsi/initiator
consolidation "osnet"
classification "System/Hardware"
import SUNWiscsiu
import SUNWiscsir

add driver name=iscsi \
    perms="* 0600 root sys" \
    class=scsi-self-identify

chattr kernel/drv/iscsi.conf \
	original_name=SUNWiscsi:kernel/drv/iscsi.conf
end package

package SUNWiscsi
consolidation "osnet"
renamed 133
depend network/iscsi/initiator
end package
