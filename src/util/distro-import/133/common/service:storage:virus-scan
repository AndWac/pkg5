package service/storage/virus-scan
consolidation "osnet"
classification "System/Services"
import SUNWvscankr
import SUNWvscanu
import SUNWvscanr

add driver name=vscan \
    perms="* 0640 root sys"
end package

package SUNWvscan
consolidation "osnet"
renamed 133
depend service/storage/virus-scan
end package
