	target=../../../../sfw/share/man/man1/gcc.1
add link path=usr/gnu/share/man/man1/cpp.1 \
	target=../../../../sfw/share/man/man1/cpp.1
add dir path=usr/share mode=0755 owner=root group=sys
add dir path=usr/share/man mode=0755 owner=root group=bin
add dir path=usr/share/man/man1 mode=0755 owner=root group=bin
add link path=usr/share/man/man1/g++.1 target=../../../sfw/share/man/man1/g++.1
add link path=usr/share/man/man1/g77.1 target=../../../sfw/share/man/man1/g77.1
