	original_name=SUNWGtk:etc/gtk/gtkrc.he
chattr etc/gtk/gtkrc.hy \
	original_name=SUNWGtk:etc/gtk/gtkrc.hy
chattr etc/gtk/gtkrc.iso-8859-13 \
	original_name=SUNWGtk:etc/gtk/gtkrc.iso-8859-13
