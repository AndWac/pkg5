package network/iscsi
consolidation "osnet"
classification "System/Hardware"
import SUNWstmf
import SUNWstmfu

add driver name=fct \
    perms="* 0666 root sys"

add driver name=pppt \
    perms="* 0666 root sys"

add driver name=qlt \
    perms="* 0666 root sys"

add driver name=stmf \
    perms="* 0666 root sys"

add driver name=stmf_sbd \
    perms="* 0666 root sys"
end package

package SUNWstmf
consolidation "osnet"
renamed 133
depend network/iscsi
end package
