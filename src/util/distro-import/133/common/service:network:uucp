package service/network/uucp
consolidation "osnet"
classification "System/Services"
import SUNWbnuu
import SUNWbnur

chattr etc/init.d/uucp \
	original_name=SUNWbnu:etc/init.d/uucp
chattr etc/uucp/Config \
	original_name=SUNWbnu:etc/uucp/Config
chattr etc/uucp/Devconfig \
	original_name=SUNWbnu:etc/uucp/Devconfig
chattr etc/uucp/Devices \
	original_name=SUNWbnu:etc/uucp/Devices
chattr etc/uucp/Dialcodes \
	original_name=SUNWbnu:etc/uucp/Dialcodes
chattr etc/uucp/Dialers \
	original_name=SUNWbnu:etc/uucp/Dialers
chattr etc/uucp/Grades \
	original_name=SUNWbnu:etc/uucp/Grades
chattr etc/uucp/Limits \
	original_name=SUNWbnu:etc/uucp/Limits
chattr etc/uucp/Permissions \
	original_name=SUNWbnu:etc/uucp/Permissions
chattr etc/uucp/Poll \
	original_name=SUNWbnu:etc/uucp/Poll
chattr etc/uucp/Sysfiles \
	original_name=SUNWbnu:etc/uucp/Sysfiles
chattr etc/uucp/Systems \
	original_name=SUNWbnu:etc/uucp/Systems
end package

package SUNWbnu
consolidation "osnet"
renamed 133
depend service/network/uucp
end package
