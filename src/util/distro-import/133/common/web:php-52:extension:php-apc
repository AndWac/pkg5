package web/php-52/extension/php-apc
consolidation "sfw"
classification "Development/PHP"
import SUNWphp52r-apc
import SUNWphp52u-apc
depend web/php-52
version 3.0.19

chattr etc/apache2/2.2/samples-conf.d/php-apc-stats.conf \
	original_name=SUNWphp52-apc:etc/apache2/2.2/samples-conf.d/php-apc-stats.conf
chattr etc/php/5.2/conf.d/apc.ini \
	original_name=SUNWphp52-apc:etc/php/5.2/conf.d/apc.ini
chattr etc/php/5.2/zts-conf.d/apc.ini \
	original_name=SUNWphp52-apc:etc/php/5.2/zts-conf.d/apc.ini
end package

package SUNWphp52-apc
consolidation "sfw"
renamed 133
depend web/php-52/extension/php-apc
version 3.0.19
end package
