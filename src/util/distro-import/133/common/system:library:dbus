package system/library/dbus
consolidation "gnome"
classification "System/Services"
import SUNWdbus
import SUNWdbus-root

chattr etc/dbus-1/session.conf \
	original_name=SUNWdbus:etc/dbus-1/session.conf
chattr etc/dbus-1/system.conf \
	original_name=SUNWdbus:etc/dbus-1/system.conf
end package

package SUNWdbus
consolidation "gnome"
renamed 133
depend system/library/dbus
end package
