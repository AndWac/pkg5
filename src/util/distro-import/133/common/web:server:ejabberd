package web/server/ejabberd
consolidation "sfw"
classification "Web Services/Communications"
import SUNWejabberdr
import SUNWejabberdu
depend runtime/erlang
version 2.0.5

chattr etc/ejabberd/ejabberd.cfg \
	original_name=SUNWejabberd:etc/ejabberd/ejabberd.cfg
chattr etc/ejabberd/ejabberdctl.cfg \
	original_name=SUNWejabberd:etc/ejabberd/ejabberdctl.cfg
chattr etc/ejabberd/inetrc \
	original_name=SUNWejabberd:etc/ejabberd/inetrc
end package

package SUNWejabberd
consolidation "sfw"
renamed 133
depend web/server/ejabberd
version 2.0.5
end package
