package office/openoffice
classification "Applications/Office"
summary "OpenOffice.org 3.2.0"
version 3.2.0
import ooobasis32-base
import ooobasis32-binfilter
import ooobasis32-calc
import ooobasis32-core01
import ooobasis32-core02
import ooobasis32-core03
import ooobasis32-core04
import ooobasis32-core05
import ooobasis32-core06
import ooobasis32-core07
import ooobasis32-draw
import ooobasis32-en-US
import ooobasis32-en-US-base
import ooobasis32-en-US-binfilter
import ooobasis32-en-US-calc
import ooobasis32-en-US-draw
import ooobasis32-en-US-help
import ooobasis32-en-US-impress
import ooobasis32-en-US-math
import ooobasis32-en-US-res
import ooobasis32-en-US-writer
import ooobasis32-gnome-integratn
import ooobasis32-graphicfilter
import ooobasis32-images
import ooobasis32-impress
import ooobasis32-javafilter
import ooobasis32-math
import ooobasis32-ooofonts
import ooobasis32-oooimprovement
import ooobasis32-ooolinguistic
import ooobasis32-pyuno
import ooobasis32-testtool
import ooobasis32-writer
import ooobasis32-xsltfilter
import openofficeorg3
import openofficeorg3-desktop-int
drop etc/mailcap
drop etc/mime.types
drop share/mime
drop share/mime/packages
import openofficeorg3-base
import openofficeorg3-calc
import openofficeorg3-dict-en
import openofficeorg3-draw
import openofficeorg3-en-US
import openofficeorg3-impress
import openofficeorg3-math
import openofficeorg3-writer
import openofficeorg-ure
add depend fmri=openoffice@3.1.0-1 type=optional
end package

