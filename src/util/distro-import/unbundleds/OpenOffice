package office/openoffice
classification "Applications/Office"
summary "OpenOffice.org Office Productivity Suite"
version 3.3.0
import ooobasis33-base
import ooobasis33-binfilter
import ooobasis33-calc
import ooobasis33-core01
import ooobasis33-core02
import ooobasis33-core03
import ooobasis33-core04
import ooobasis33-core05
import ooobasis33-core06
import ooobasis33-core07
import ooobasis33-draw
import ooobasis33-en-US
import ooobasis33-en-US-base
import ooobasis33-en-US-binfilter
import ooobasis33-en-US-calc
import ooobasis33-en-US-draw
import ooobasis33-en-US-help
import ooobasis33-en-US-impress
import ooobasis33-en-US-math
import ooobasis33-en-US-res
import ooobasis33-en-US-writer
import ooobasis33-gnome-integratn
import ooobasis33-graphicfilter
import ooobasis33-images
import ooobasis33-impress
import ooobasis33-javafilter
import ooobasis33-math
import ooobasis33-ooofonts
import ooobasis33-oooimprovement
import ooobasis33-ooolinguistic
import ooobasis33-pyuno
import ooobasis33-testtool
import ooobasis33-writer
import ooobasis33-xsltfilter
import openofficeorg3
import openofficeorg3-desktop-int
drop etc/mailcap
drop etc/mime.types
drop share/mime
drop share/mime/packages
import openofficeorg-ure
import openofficeorg3-base
import openofficeorg3-calc
import openofficeorg3-dict-en
import openofficeorg3-draw
import openofficeorg3-en-US
import openofficeorg3-impress
import openofficeorg3-math
import openofficeorg3-writer
add depend fmri=openoffice@3.1.0-1 type=optional
# add actuators for desktop files under /opt as these are not picked up
# by the default global rules
chattr_glob *.desktop type file \
    restart_fmri=svc:/application/desktop-cache/desktop-mime-cache:default
end package

