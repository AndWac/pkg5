add depend type=incorporate fmri=SUNWlibnb-profiler2@6.1
add depend type=incorporate fmri=SUNWlibnb-ruby1@6.1
add depend type=incorporate fmri=SUNWlibnb-visualweb1@6.1
add depend type=incorporate fmri=SUNWlibnb-xml1@6.1
add depend type=incorporate fmri=SUNWnetbeans-cpp@6.1
add depend type=incorporate fmri=SUNWnetbeans-desktop@6.1
