package SUNWnamow
import SUNWnamow exclude usr/openwin
drop usr/openwin/share
drop usr/openwin/share/locale
drop usr/openwin/share/locale/en_CA.ISO8859-1
drop usr/openwin/share/locale/en_CA.UTF-8
drop usr/openwin/share/locale/en_US
drop usr/openwin/share/locale/en_US.ISO8859-1
drop usr/openwin/share/locale/en_US.ISO8859-15
drop usr/openwin/share/locale/es_MX.ISO8859-1
drop usr/openwin/share/locale/es_MX.UTF-8
drop usr/openwin/share/locale/fr_CA.ISO8859-1
drop usr/openwin/share/locale/fr_CA.UTF-8
chattr_glob * edit path usr/openwin/lib usr/X11/share/X11
add link path=usr/X11/share/X11/locale/en_US target=./en_US.ISO8859-1
end package
