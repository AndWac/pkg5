package SUNWcs
import SUNWcsu
# BEGIN NON-REDISTRIBUTABLE
drop usr/bin/ksh
drop usr/bin/pfksh
drop usr/bin/rksh
add link path=usr/bin/rksh target=rksh93
# END NON-REDISTRIBUTABLE
# /usr/has
add dir path=usr/has mode=0755 owner=root group=sys
add dir path=usr/has/bin mode=0755 owner=root group=sys
add dir path=usr/has/man mode=0755 owner=root group=sys
add dir path=usr/has/man/man1 mode=0755 owner=root group=sys
# Replace BSD vi with vim
drop usr/bin/ex
drop usr/bin/vi
drop usr/bin/view
add link path=usr/has/man/man1/vi.1 target=../../../share/man/missing.man
add link path=usr/has/man/man1/ex.1 target=../../../share/man/missing.man
add link path=usr/has/man/man1/view.1 target=../../../share/man/missing.man
# hack until clofi integrates into nevada
drop usr/sbin/lofiadm
from SUNWclofi import
usr/sbin/lofiadm
end import
import SUNWcsr
drop var/svc/profile/generic_open.xml
drop var/svc/profile/generic_limited_net.xml
drop etc/inet/hosts
drop etc/nsswitch.conf
from ./SUNWfixes import
# until the necessary actions are available
etc/inet/services
etc/security/auth_attr
etc/security/prof_attr
# switch to gdm
var/svc/profile/generic_open.xml
var/svc/profile/generic_limited_net.xml
# customizations for indiana
etc/inet/hosts
etc/nsswitch.conf
etc/user_attr
etc/security/exec_attr
root
usr/bin/ksh
usr/has/bin/ex
usr/has/bin/vi
usr/has/bin/view
end import
end package
